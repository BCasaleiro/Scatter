package  {		public class GameEngine {		var game : Main;		var map : Map;		var level : Level;		public function GameEngine(game : Main) {			this.game = game;		}		public function start() {			game.clean();			this.map.generateMap(level);			this.game.addChild(this.map);		}				public function stop() {					}				public function setMap(map : Map) {			this.map = map;		}				public function setLevel(level : Level) {			this.level = level;		}	}	}